{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spofitydatafactory"
		},
		"AzureSql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSql_ls'"
		},
		"AzureStorage_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureStorage_ls'"
		},
		"AzureSql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyhs.database.windows.net"
		},
		"AzureSql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifydb"
		},
		"AzureSql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hasansql"
		},
		"AzureStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifydatalakegen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spofitydatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSql_ls_password": {
						"value": "[parameters('AzureSql_ls_password')]"
					},
					"AzureStorage_ls_accountKey": {
						"value": "[parameters('AzureStorage_ls_accountKey')]"
					},
					"AzureSql_ls_properties_typeProperties_server": {
						"value": "[parameters('AzureSql_ls_properties_typeProperties_server')]"
					},
					"AzureSql_ls_properties_typeProperties_database": {
						"value": "[parameters('AzureSql_ls_properties_typeProperties_database')]"
					},
					"AzureSql_ls_properties_typeProperties_userName": {
						"value": "[parameters('AzureSql_ls_properties_typeProperties_userName')]"
					},
					"AzureStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureStorage_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}